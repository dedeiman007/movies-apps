            </svg>\r\n                {{ movie.vote_average }}\r\n            </div>\r\n        </div>\r\n        <div class=\"movie-action\">\r\n            <ButtonAddToList type=\"movie\" :movie=\"movie\" :short=\"true\" @removeFromList=\"removeFromList\"/>\r\n        </div>\r\n        <div class=\"movie-detail\" @click=\"showDetail()\">\r\n            <div class=\"movie-title\">\r\n                {{ movie.title }}\r\n            </div>\r\n            <div class=\"movie-release-date\">\r\n                {{ date(movie.release_date, 'll') }}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport ButtonAddToList from './ButtonAddToList.vue';\r\nexport default {\r\n    components: {\r\n        ButtonAddToList,\r\n    },\r\n    props: {\r\n        movie: {\r\n            type: Object\r\n        },\r\n        imageConfiguration: {\r\n            type: Object\r\n        },\r\n    },\r\n    data() {\r\n        return {\r\n            visible: true\r\n        }\r\n    },\r\n    methods: {\r\n        generateImageUrl(image) {\r\n            var config = this.imageConfiguration\r\n            return config.base_url + config.poster_sizes[2] + image\r\n        },\r\n        showDetail() {\r\n            this.$router.push({\r\n                name: 'Show',\r\n                params: {\r\n                    id: this.movie.id\r\n                }\r\n            })\r\n        },\r\n        removeFromList() {\r\n            if(this.$route.name == 'MyList') {\r\n                this.$emit('removeFromList', this.movie)\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"]}]}